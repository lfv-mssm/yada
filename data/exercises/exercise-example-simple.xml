    <TerminalLink terminalid="1" groupid="1">
        <PlayerRef id="1"/>
    </TerminalLink>

    <TerminalLink terminalid="2" groupid="1">
        <PlayerRef id="2"/>
    </TerminalLink>

    <TerminalLink terminalid="3" groupid="1">
        <PlayerRef id="3"/>
    </TerminalLink>

</Exercise>
